
<h3> Sigu Up Here </h3> 

<%= form_for @user do |f| %> 
    
        <%= f.label :email %>
        <%= f.text_field :email %>

        <%= f.label :username %>
        <%= f.text_field :username %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit 'Sign Up' %>
    
    <% end %>